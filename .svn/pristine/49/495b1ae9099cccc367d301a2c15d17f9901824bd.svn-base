<Window x:Class="Engine.Views.WorldMapView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Engine.Views"
             mc:Ignorable="d" 
             Height="450" Width="800">
    <Grid Background="Beige">
        <Grid.RowDefinitions>
            <RowDefinition Height="9*"/>
            <RowDefinition />
        </Grid.RowDefinitions>
        <Image Grid.Row="0" HorizontalAlignment="Center" Source="/Engine;component/Images/Locations/Worldmap.png" />
        <Button Content="Close" Grid.Row="1" Width="50" Height="25"  HorizontalAlignment="Center" Command="{Binding CommandOk}"/>
    </Grid>
</Window>
